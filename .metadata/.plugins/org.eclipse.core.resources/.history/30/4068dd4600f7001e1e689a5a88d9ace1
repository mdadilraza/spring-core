package com.jsp.springboot_day1;

public class Employee {
	int id;
	String name;
	public Employee(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	
	public Employee() {
		super();
	}

	public static void main(String[] args) {
		Employee employee= new Employee();
		System.out.println(employee);
	}

}
